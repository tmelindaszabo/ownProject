{
    "compilerOptions": {
      "target": "es6" ,
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./", 
      "strict": true,
      "noImplicitAny": true, 
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }